.wrap
  .collection-container
    %a{name:'collection'}
    %h1
      Past Vehicle Archive
    .vehicle-list-container
      - data.vehicles.vehicles.each do |vehicle|
        -if vehicle.current == "false"
          =link_to("/#{vehicle.photopath}", class:'vehicle-list-item', id:"#{vehicle.indexx}") do
            =image_tag("#{vehicle.photopath}/cutout/cutout.jpg", class:'vehicle-list-cutout')
            .year-make-model-vlp
              #{vehicle.year} #{vehicle.make}
              %br
              %span
                =vehicle.model
            .line
            .logo-container
              .logo{class:"#{vehicle.make}"}
              .logo{class:"#{vehicle.model}"}
    =link_to 'View Current Collection', '/#collection', class:'archive-cta'